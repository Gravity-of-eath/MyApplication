<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@color/black"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="10dp">

        <testoffpro.hmj.yibin.testoffpro.customview.RoundImageView.RoundAngleImageView
            android:id="@+id/user_icon"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:scaleType="centerCrop"
            app:round="30dp"
            android:src="@drawable/pic_banner"
            android:background="@drawable/pic_user" />

        <TextView
            android:id="@+id/user_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:text="@string/username"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_16" />

        <TextView
            android:id="@+id/user_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:text="@string/email"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_16" />

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/persional_next"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:padding="10dp"
                android:src="@drawable/icon_more2" />

            <TextView
                android:id="@+id/version"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:text="V1.11"
                android:textColor="@color/list_devider_color" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:background="@color/white"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/personinfo_next"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_hight"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@drawable/icon_u_n1" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_l"
                android:text="@string/personinfo"
                android:textSize="@dimen/textsize" />

            <TextView
                android:id="@+id/personinfo_note"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="xx"
                android:textColor="@color/orange_color"
                android:visibility="invisible" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:src="@drawable/icon_more2" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="@dimen/devider_w"
            android:background="@color/list_devider_color"></View>

        <LinearLayout
            android:id="@+id/my_order_next"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_hight"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@drawable/icon_u_n2" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_l"
                android:text="@string/my_order"
                android:textSize="@dimen/textsize" />

            <TextView
                android:id="@+id/my_order_note"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="xx"
                android:textColor="@color/orange_color"
                android:visibility="invisible" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:src="@drawable/icon_more2" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="@dimen/devider_w"
            android:background="@color/list_devider_color"></View>

        <LinearLayout
            android:id="@+id/my_financial_next"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_hight"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@drawable/icon_u_n3" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_l"
                android:text="@string/my_financial"
                android:textSize="@dimen/textsize" />

            <TextView
                android:id="@+id/my_financial_note"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="xx"
                android:textColor="@color/orange_color"
                android:visibility="invisible" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:src="@drawable/icon_more2" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:background="@color/list_devider_color"></View>

        <LinearLayout
            android:id="@+id/bind_email_next"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_hight"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@drawable/icon_u_n4" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_l"
                android:text="@string/bind_email"
                android:textSize="@dimen/textsize" />

            <TextView
                android:id="@+id/bind_email_note"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="@string/no_bind"
                android:textColor="@color/orange_color" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:src="@drawable/icon_more2" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="@dimen/devider_w"
            android:background="@color/list_devider_color"></View>

        <LinearLayout
            android:id="@+id/c_c_s_next"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_hight"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@drawable/icon_u_n5" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_l"
                android:text="@string/c_c_s"
                android:textSize="@dimen/textsize" />

            <TextView
                android:id="@+id/c_c_s_note"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="@string/unread"
                android:textColor="@color/orange_color" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:src="@drawable/icon_more2" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="@dimen/devider_w"
            android:background="@color/list_devider_color"></View>

        <LinearLayout
            android:id="@+id/invite_friend_next"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_hight"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@drawable/icon_u_n6" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_l"
                android:text="@string/invite_friend"
                android:textSize="@dimen/textsize" />

            <TextView
                android:id="@+id/invite_friend_note"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="xx"
                android:textColor="@color/orange_color"
                android:visibility="invisible" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:src="@drawable/icon_more2" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="@dimen/devider_w"
            android:background="@color/list_devider_color"></View>

        <LinearLayout
            android:id="@+id/my_message_next"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_hight"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@drawable/icon_u_n7" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_l"
                android:text="@string/my_message"
                android:textSize="@dimen/textsize" />

            <TextView
                android:id="@+id/my_message_note"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="xx"
                android:textColor="@color/orange_color"
                android:visibility="invisible" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:src="@drawable/icon_more2" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="@dimen/devider_w"
            android:background="@color/list_devider_color"></View>

        <LinearLayout
            android:id="@+id/my_rcv_address_next"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_hight"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@drawable/icon_u_n8" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_l"
                android:text="@string/my_rcv_address"
                android:textSize="@dimen/textsize" />

            <TextView
                android:id="@+id/my_rcv_address_note"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="xx"
                android:textColor="@color/orange_color"
                android:visibility="invisible" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:src="@drawable/icon_more2" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="@dimen/devider_w"
            android:background="@color/list_devider_color"></View>

        <LinearLayout
            android:id="@+id/common_problems_next"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_hight"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@drawable/icon_u_n9" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_l"
                android:text="@string/common_problems"
                android:textSize="@dimen/textsize" />

            <TextView
                android:id="@+id/common_problems_note"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:text="xx"
                android:textColor="@color/orange_color"
                android:visibility="invisible" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:src="@drawable/icon_more2" />

        </LinearLayout>

        <Button
            android:id="@+id/persional_exit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:background="@drawable/loginbtn_bg"
            android:text="@string/exit_"
            android:textColor="@color/white" />

    </LinearLayout>

</LinearLayout>